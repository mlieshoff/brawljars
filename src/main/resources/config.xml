<config basePackage="brawljars" package="brawljars.model">
    <for type="CLASS">
        <template id="PojoClass" name="pojo.vm" suffix=".java"/>
        <template id="PojoTest" name="pojo-test.vm" suffix="Test.java" test="true"/>
    </for>
    <for type="ENUM">
        <template id="EnumClass" name="enum.vm" suffix=".java"/>
        <template id="EnumTest" name="enum-test.vm" suffix="Test.java" test="true"/>
    </for>
</config>