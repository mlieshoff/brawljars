package ${helper.getPackageName($properties)};

import java.io.IOException;
import brawljars.v2.api.AbstractApi;
import brawljars.v2.api.ApiContext;
import brawljars.v2.api.Connector;
import brawljars.v2.api.RequestContext;

class ${filename} extends AbstractApi implements ${WordUtils.capitalize($object.name)}Api {

  public ${filename}(ApiContext apiContext) {
    super(apiContext);
  }

  @Override
  #foreach( $method in $object.method )
  public ${helper.getTypeName($method.return)} ${helper.getGetterMethodName($method.name)}(#foreach( $attribute in $method.attribute )${helper.getTypeName($attribute.type)} ${helper.getAttributeName($attribute.type)}#end) throws IOException {
    String apiKey = getApiContext().getApiKey();
    Connector connector = getApiContext().getConnector();
    String url = createUrl("/${object.name.toLowerCase()}", #foreach($attribute in $method.attribute)${helper.getAttributeName($attribute.type)}#end);
    RequestContext requestContext = new RequestContext(url, apiKey, #foreach($attribute in $method.attribute)${helper.getAttributeName($attribute.type)}#end, ${helper.getTypeName($method.return)}.class);
    return connector.get(requestContext);
  }

#end
}